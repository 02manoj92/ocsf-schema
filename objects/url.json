{
  "name": "Uniform Resource Locator (URL)",
  "type": "url",
  "description": "The Uniform Resource Locator (URL) object describes the path and reputation of a URL.",
  "attributes": {
    "categories": {
      "requirement": "optional"
    },
    "category_ids": {
      "requirement": "recommended"
    },
    "hostname": {
      "description": "The URL host as extracted from the URL. For example: www.example.com.",
      "requirement": "required"
    },
    "method": {
      "requirement": "recommended"
    },
    "parent_categories": {
      "requirement": "recommended"
    },
    "path": {
      "description": "The URL path as extracted from the URL. For example: <code>/download/trouble</code>.",
      "requirement": "required"
    },
    "port": {
      "description": "The URL port. For example: <code>80</code>.",
      "requirement": "recommended"
    },
    "provider": {
      "requirement": "recommended"
    },
    "http_query": {
      "requirement": "recommended"
    },
    "referrer": {
      "requirement": "optional"
    },
    "rep_score": {
      "requirement": "optional"
    },
    "rep_score_id": {
      "requirement": "recommended"
    },
    "scheme": {
      "requirement": "recommended"
    },
    "text": {
      "description": "The URL. For example: www.example.com/download/trouble.exe.",
      "requirement": "required"
    }
  }
}