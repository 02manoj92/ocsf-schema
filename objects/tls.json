{
  "caption": "Transport Layer Security (TLS)",
  "name": "tls",
  "description": "The negotiated TLS protocol used for secure communications over an establish network connection.",
  "extends": "object",
  "attributes": {
    "alert": {
      "requirement": "optional"
    },
    "certificate": {
      "requirement": "required"
    },
    "cipher": {
      "requirement": "recommended"
    },
    "client_ciphers": {
      "requirement": "recommended"
    },
    "sni": {
      "requirement": "required"
    },
    "handshake_time": {
      "requirement": "optional"
    },
    "key_length": {
      "requirement": "optional"
    },
    "server_ciphers": {
      "requirement": "optional"
    },
    "tls_extension": {
      "requirement": "optional"
    },
    "version": {
      "description": "The TLS protocol version.",
      "caption": "Version",
      "requirement": "required"
    }
  }
}
