{
  "name": "Transport Layer Security (TLS)",
  "type": "tls",
  "description": "The negotiated TLS protocol used for secure communications over an establish network connection.",
  "attributes": {
    "alert": {
      "requirement": "optional"
    },
    "cert_serial": {
      "requirement": "optional"
    },
    "cert_uid": {
      "requirement": "recommended"
    },
    "cipher": {
      "requirement": "optional"
    },
    "client_ciphers": {
      "requirement": "optional"
    },
    "domain": {
      "description": "The domain name used in the TLS negotiation.",
      "requirement": "recommended"
    },
    "handshake_time": {
      "requirement": "optional"
    },
    "key_length": {
      "requirement": "optional"
    },
    "server_ciphers": {
      "requirement": "optional"
    },
    "version": {
      "description": "The TLS protocol version.",
      "name": "Version",
      "requirement": "required"
    }
  }
}
