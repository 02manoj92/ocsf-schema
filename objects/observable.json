{
  "name": "Observable",
  "type": "observable",
  "description": "The observable object is a pivot element that contains related information found in many places in the event.",
  "extends": "object",
  "attributes": {
    "name": {
      "description": "The name of the observable attribute. For example: <code>file.name</code>.",
      "requirement": "required"
    },
    "role_ids": {
      "enum": {
        "0": {
          "name": "Unknown",
          "description": "Unknown role."
        },
        "1": {
          "name": "Actor"
        },
        "2": {
          "name": "Target"
        },
        "3": {
          "name": "Attacker"
        },
        "4": {
          "name": "Victim"
        },
        "5": {
          "name": "Parent Process"
        },
        "6": {
          "name": "Child Process"
        },
        "7": {
          "name": "Known Bad"
        },
        "8": {
          "name": "Data Loss"
        },
        "9": {
          "name": "Observer"
        },
        "-1": {
          "name": "Other",
          "description": "The observable role is not mapped."
        }
      }
    },
    "roles": {
    },
    "type": {
      "description": "The observable value type name.",
      "requirement": "required"
    },
    "type_id": {
      "description": "The observable value type identifier.",
      "requirement": "required",
      "enum": {
        "0": {
          "name": "Unknown",
          "description": "Unknown observable data type."
        },
        "-1": {
          "name": "Other",
          "description": "The observable data type is not mapped. See the <code>type</code> attribute, which may contain data source specific value."
        }
      }
    },
    "value": {
      "description": "The value associated with the observable attribute. The meaning of the data depends on the observable type.",
      "requirement": "optional"
    }
  }
}
