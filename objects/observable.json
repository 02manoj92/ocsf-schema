{
  "name": "Observable",
  "type": "observable",
  "description": "The observable object is a pivot element that contains related information found in many places in the event.",
  "attributes": {
    "name": {
      "description": "The name of the observable attribute. For example: `file.name`",
      "requirement": "required"
    },
    "type": {
      "description": "The observable value type name that is not in the pre-defined types.",
      "requirement": "optional"
    },
    "value": {
      "description": "The data value associated with the observable attribute. The meaning of this data depends on the observable type.",
      "requirement": "required"
    },
    "ref_uid": {},
    "role_ids": {
      "requirement": "required"
    },
    "roles": {
      "requirement": "optional"
    },
    "type_id": {
      "type": "integer_t",
      "description": "The observable value type identifier.",
      "requirement": "required",
      "enum": {
        "0": {
          "name": "Unknown",
          "description": "Unknown observable data type."
        },
        "1": {
          "name": "Device"
        },
        "2": {
          "name": "Container"
        },
        "3": {
          "name": "Endpoint"
        },
        "4": {
          "name": "Hostname"
        },
        "5": {
          "name": "IP Address"
        },
        "6": {
          "name": "User"
        },
        "7": {
          "name": "Username"
        },        
        "8": {
          "name": "Email"
        },
        "9": {
          "name": "Email Address"
        },
        "10": {
          "name": "URL"
        },
        "11": {
          "name": "URL Domain"
        },
        "12": {
          "name": "File"
        },
        "13": {
          "name": "File Name"
        },
        "14": {
          "name": "File Hash"
        },
        "15": {
          "name": "Process"
        },
        "16": {
          "name": "Process Name"
        },
        "17": {
          "name": "Location"
        },
        "-1": {
          "name": "Other",
          "description": "The observable data type is not mapped. See the <code>type</code> attribute, which may contain data source specific value."
        }
      }
    }
  }
}