{
    "description": "The CVSS object details Common Vulnerability Scoring System (CVSS) scores from the advisory that are related to the vulnerability.",
    "name": "CVSS Scores",
    "type": "cvss",
    "attributes": {
        "access_complexity_id": {
            "requirement": "optional",
            "enum": {
                "0": {
                    "name": "Unknown"
                },
                "1": {
                    "name": "Low"
                },
                "2": {
                    "name": "Medium"
                },
                "3": {
                    "name": "High"
                }
            }
        },
        "attack_vector_id": {
            "requirement": "optional",
            "enum": {
                "0": {
                    "name": "Unknown"
                },
                "1": {
                    "name": "Local Access"
                },
                "2": {
                    "name": "Adjacent Network Access"
                },
                "3": {
                    "name": "Network Accessible"
                }
            }
        },
        "authentication_id": {
            "requirement": "optional",
            "enum": {
                "0": {
                    "name": "Unknown"
                },
                "1": {
                    "name": "Multiple Instances"
                },
                "2": {
                    "name": "Single Instance"
                },
                "3": {
                    "name": "No Authentication"
                }
            }
        },
        "availability_impact_id": {
            "requirement": "optional",
            "enum": {
                "0": {
                    "name": "Unknown"
                },
                "1": {
                    "name": "No Impact"
                },
                "2": {
                    "name": "Partial"
                },
                "3": {
                    "name": "Complete"
                }
            }
        },
        "base_score": {
            "requirement": "recommended"
        },
        "base_vector": {
            "requirement": "recommended"
        },
        "confidentiality_impact_id": {
            "requirement": "optional",
            "enum": {
                "0": {
                    "name": "Unknown"
                },
                "1": {
                    "name": "No Impact"
                },
                "2": {
                    "name": "Partial"
                },
                "3": {
                    "name": "Complete"
                }
            }
        },
        "integrity_impact_id": {
            "requirement": "optional",
            "enum": {
                "0": {
                    "name": "Unknown"
                },
                "1": {
                    "name": "No Impact"
                },
                "2": {
                    "name": "Partial"
                },
                "3": {
                    "name": "Complete"
                }
            }
        },
        "risk": {
            "requirement": "optional"
        },
        "version": {
            "description": "The CVSS version",
            "requirement": "recommended"
        }
    }
}