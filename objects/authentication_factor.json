{
  "caption": "Authentication Factor",
  "description": "The authentication factor object provides information about the factor used to authenticate in an mfa request.",
  "name": "authentication factor",
  "extends": "object",
  "attributes": {
    "factor_device": {
      "description": "Device used to complete the authentication request.",
      "requirement": "recommended"
    },
    "factor_type": {
      "enum": {
        "1": {
          "caption": "SMS"
        },
        "2 question": {
          "caption": "Security Question"
        },
        "3": {
          "caption": "Call"
        },
        "4": {
          "caption": "Time-based One-time Password (totp)"
        },
        "5": {
          "caption": "Push Factor"
        },
        "6": {
          "caption": "Token"
        },
        "7": {
          "caption": "Security Key"
        },
        "8": {
          "caption": "Email"
        },
        "9": {
          "caption": "U2F"
        },
        "10": {
          "caption": "WebAuthn"
        }
      },
      "requirement": "required"
    },
    "factor_question": {
      "description": "Question provided for user to answer for question based authentication factor.",
      "requirement": "optional"
    },
    "factor_telephony_num": {
      "description": "Phone number for then telephony based mfa request.",
      "requirement": "optional"
    },
    "factor_email": {
      "description": "Email that an authentication was performed with",
      "requirement": "optional"
    },
    "factor_provider": {
      "description": "Provider of mfa Factor",
      "requirement": "recommended"
    },
    "outcome": {
      "enum": {
        "1": {
          "description": "Authentication attempt was approved.",
          "caption": "Authentication Allowed"
        },
        "2": {
          "description": "Authentication attempt was denied.",
          "caption": "Authentication Denied"
        }
      },
      "description": "Outcome for user's authentication attempt.",
      "requirement": "recommended"
    }
  },
  "just_one": [
    "factor_email",
    "factor_telephony_num",
    "factor_question"
  ]
}
