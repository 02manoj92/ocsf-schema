{
  "caption": "Authentication Factor",
  "description": "Information on factors used to authenticate.",
  "name": "authentication factor",
  "extends": "object",
  "attributes": {
    "factor_device": {
      "description": "Device used to complete the authentication request.",
      "requirement": "recommended"
    },
    "factor_type": {
      "enum": {
        "0": {
          "caption": "Unknown"
        },
        "1": {
          "caption": "SMS"
        },
        "2 question": {
          "caption": "Security Question"
        },
        "3": {
          "caption": "Phone Call"
        },
        "4": {
          "caption": "Time-based One-time Password (totp)"
        },
        "5": {
          "caption": "Push Notification"
        },
        "6": {
          "caption": "Token"
        },
        "7": {
          "caption": "Security Key"
        },
        "8": {
          "caption": "Email"
        },
        "9": {
          "caption": "U2F"
        },
        "10": {
          "caption": "WebAuthn"
        },
        "99": {
          "caption": "Other"
        }
      },
      "requirement": "required"
    },
    "factor_question": {
      "description": "Question provided for user to answer for question based authentication factor.",
      "requirement": "optional"
    },
    "factor_telephony_num": {
      "description": "Phone number for then telephony based MFA request.",
      "requirement": "optional"
    },
    "factor_email": {
      "description": "Email that an authentication was performed with",
      "requirement": "optional"
    },
    "factor_provider": {
      "description": "Provider of MFA Factor",
      "requirement": "recommended"
    }
  },
  "just_one": [
    "factor_email",
    "factor_telephony_num",
    "factor_question"
  ]
}
