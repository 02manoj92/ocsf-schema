{
  "name": "Network Interface",
  "type": "network_interface",
  "description": "The network interface object describes the type, reputation, and associated addresses of a network interface.",
  "attributes": {
    "gateway_ip": {
      "requirement": "optional"
    },
    "gateway_mac": {
      "requirement": "optional"
    },
    "hostname": {
      "description": "The hostname associated with the network interface.",
      "requirement": "recommended"
    },
    "ip": {
      "description": "The IP address associated with the network interface.",
      "requirement": "recommended"
    },
    "mac": {
      "description": "The MAC address of the network interface.",
      "requirement": "recommended"
    },
    "name": {
      "description": "The name of the network interface.",
      "requirement": "recommended"
    },
    "namespace": {
      "requirement": "optional"
    },
    "rep_score": {
      "requirement": "optional"
    },
    "rep_score_id": {
      "requirement": "optional"
    },
    "ssid": {
      "requirement": "optional"
    },
    "type": {
      "description": "The type of network interface.",
      "requirement": "required"
    },
    "type_id": {
      "description": "The network interface type identifier.",
      "enum": {
        "-1": {
          "name": "Other"
        },
        "0": {
          "name": "Unknown"
        },
        "1": {
          "name": "Wired"
        },
        "2": {
          "name": "Wireless"
        },
        "3": {
          "name": "Mobile"
        }
      },
      "requirement": "required"
    }
  }
}
