{
  "name": "Endpoint User",
  "type": "user_ex",
  "description": "The extendedi, endpoint user object.",
  "extends": "user",
  "attributes": {
    "domain": {
      "description": "The domain where the user is defined. For example: the LDAP or Active Directory domain.",
      "requirement": "optional"
    },
    "full_name": {
      "requirement": "optional"
    },
    "home_dir": {
      "requirement": "optional"
    },
    "privileges": {
      "description": "The user privileges.",
      "requirement": "optional"
    },
    "risk_score": {
      "requirement": "optional"
    },
    "risk_score_id": {
      "requirement": "optional"
    },
    "session_uid": {
      "requirement": "optional"
    },
    "shell": {
      "requirement": "optional"
    }
  }
}
