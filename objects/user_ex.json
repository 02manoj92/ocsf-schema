{
  "name": "Extended User",
  "type": "user_ex",
  "extends": "user",
  "description": "The extended/enriched user object.",
  "attributes": {
    "full_name": {
      "requirement": "optional"
    },
    "groups": {
      "description": "The administrative groups to which the user belongs.",
      "requirement": "optional"
    },
    "home_dir": {
      "requirement": "optional"
    },
    "privileges": {
      "description": "The user privileges.",
      "requirement": "optional"
    },
    "risk_level": {
      "requirement": "optional"
    },
    "risk_level_id": {
      "requirement": "optional"
    },
    "risk_score": {
      "requirement": "optional"
    },
    "shell": {
      "requirement": "optional"
    }
  }
}