{
  "name": "HTTP Request",
  "type": "http_request",
  "description": "The HTTP Request Object details a request made to a web server.",
  "attributes": {
    "args": {
      "requirement": "optional"
    },
    "hostname": {
      "description": "The hostname. For example: www.example.com.",
      "requirement": "required"
    },
    "http_headers": {
      "requirement": "recommended"
    },
    "http_method": {
      "description": "",
      "requirement": "recommended"
    },
    "http_version": {
      "description": "",
      "requirement": "recommended"
    },
    "path": {
      "description": "The URL path as extracted from the URL. For example: <code>/download/trouble</code>.",
      "requirement": "required"
    },
    "prefix": {
      "description": "Domain prefix.",
      "requirement": "optional"
    },
    "protocol_num": {
      "requirement": "optional"
    },
    "port": {
      "description": "The port number. For example: <code>80</code>.",
      "requirement": "recommended"
    },
    "referrer": {
      "requirement": "optional"
    },
    "uid": {
      "requirement": "optional"
    },
    "user_agent": {
      "description": "",
      "requirement": "recommended"
    },
    "x_forwarded_for": {
      "requirement": "optional"
    }
  }
}
