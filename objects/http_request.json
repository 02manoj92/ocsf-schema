{
  "name": "HTTP Request",
  "type": "http_request",
  "description": "The HTTP Request Object details a request made to a web server.",
  "attributes": {
    "args": {
      "requirement": "optional"
    },
    "hostname": {
      "description": "The hostname. For example: www.example.com.",
      "requirement": "required"
    },
    "http_headers": {
      "requirement": "recommended"
    },
    "http_method": {
      "description": "",
      "requirement": "recommended"
    },
    "http_version": {
      "description": "",
      "requirement": "recommended"
    },
    "prefix": {
      "description": "Domain prefix.",
      "requirement": "optional"
    },
    "referrer": {
      "requirement": "optional"
    },
    "uid": {
      "requirement": "optional"
    },
    "user_agent": {
      "description": "",
      "requirement": "recommended"
    },
    "x_forwarded_for": {
      "requirement": "optional"
    },
    "url": {
      "description": "The URL object that pertains to the request.",
      "requirement": "required",
      "group": "primary"
    }
  }
}
