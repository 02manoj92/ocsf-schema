{
  "name": "Email Authentication",
  "type": "email_auth",
  "description": "The Email Authentication object describes the Sender Policy Framework (SPF), DomainKeys Identified Mail (DKIM) and Domain-based Message Authentication, Reporting and Conformance (DMARC) attributes of an email.",
  "attributes": {
    "dkim_domain": {
      "requirement": "required"
    },
    "dkim_id": {
      "requirement": "required",
      "enum": {
        "0": {
          "name": "Unknown"
        },
        "1": {
          "name": "Pass"
        },
        "2": {
          "name": "Fail"
        },
        "3": {
          "name": "Other"
        },
        "4": {
          "name": "None"
        }
      }
    },
    "dmarc_id": {
      "requirement": "required",
      "enum": {
        "0": {
          "name": "Unknown"
        },
        "1": {
          "name": "Pass"
        },
        "2": {
          "name": "Fail"
        },
        "3": {
          "name": "Other"
        },
        "4": {
          "name": "None"
        }
      }
    },
    "dmarc_override": {
      "requirement": "required"
    },
    "dmarc_policy_id": {
      "requirement": "required",
      "enum": {
        "0": {
          "name": "Unknown"
        },
        "1": {
          "name": "Policy Reject"
        },
        "2": {
          "name": "Policy Quarantine"
        },
        "4": {
          "name": "None"
        }
      }
    },
    "raw_header": {
      "requirement": "required"
    },
    "spf_id": {
      "requirement": "required",
      "enum": {
        "0": {
          "name": "Unknown"
        },
        "1": {
          "name": "Pass"
        },
        "2": {
          "name": "Fail"
        },
        "3": {
          "name": "Other"
        },
        "4": {
          "name": "None"
        }
      }
    }
  }
}