{
  "name": "Network Alert Trait",
  "description": "Network Alert trait defines the outcome values used by network alert events.",
  "attributes": {
    "outcome": {
      "requirement": "required",
      "group": "classification"
    },
    "outcome_id": {
      "group": "classification",
      "enum": {
        "1": {
          "name": "Allowed"
        },
        "2": {
          "name": "Blocked"
        },
        "3": {
          "name": "Dropped"
        },
        "4": {
          "name": "Deleted"
        },
        "5": {
          "name": "Isolated"
        }
      }
    }
  }
}