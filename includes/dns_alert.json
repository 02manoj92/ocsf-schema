{
  "caption": "DNS Alert Trait",
  "description": "Domain Name System (DNS) Alert trait defines attributes shared by endpoint and network DNS alert events.",
  "attributes": {
    "connection": {
      "group": "context",
      "requirement": "optional"
    },
    "outcome": {
      "requirement": "required",
      "group": "classification"
    },
    "outcome_id": {
      "group": "classification",
      "enum": {
        "1": {
          "caption": "Allowed"
        },
        "2": {
          "caption": "Blocked"
        },
        "3": {
          "caption": "Dropped"
        },
        "4": {
          "caption": "Deleted"
        },
        "5": {
          "caption": "Isolated"
        }
      }
    },
    "query": {
      "requirement": "recommended",
      "group": "primary"
    },
    "query_time": {
      "group": "occurrence",
      "requirement": "optional"
    }
  }
}
