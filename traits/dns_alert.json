{
  "name": "DNS Alert Trait",
  "description": "Domain Name System (DNS) Alert trait defines attributes shared by endpoint and network DNS alert events.",
  "attributes": {
    "connection": {
      "group": "context",
      "requirement": "optional"
    },
    "disposition_id": {
      "enum": {
        "1": {
          "name": "Allowed"
        },
        "2": {
          "name": "Blocked"
        },
        "3": {
          "name": "Dropped"
        },
        "4": {
          "name": "Deleted"
        },
        "5": {
          "name": "Isolated"
        }
      }
    },
    "query": {
      "requirement": "recommended",
      "group": "primary"
    },
    "query_time": {
      "group": "occurrence",
      "requirement": "optional"
    }
  }
}
