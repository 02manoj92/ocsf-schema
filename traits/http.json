{
  "name": "HTTP Traffic Trait",
  "description": "HTTP Traffic trait defines attributes shared by HTTP traffic and alert events.",
  "attributes": {
    "http_status": {
      "group": "primary"
    },
    "http_method": {
      "group": "primary",
      "requirement": "recommended"
    },
    "request_headers": {
      "group": "primary",
      "requirement": "optional"
    },
    "response_headers": {
      "group": "primary",
      "requirement": "optional"
    },
    "url": {
      "description": "The URL object that pertains to the event.",
      "group": "primary",
      "requirement": "required"
    }
  }
}