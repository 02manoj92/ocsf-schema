{
  "name": "DNS Trait",
  "description": "Domain Name System (DNS) trait defines attributes shared by endpoint and network DNS events.",
  "attributes": {
    "response_time": {
      "requirement": "recommended"
    },
    "answers": {
      "requirement": "recommended"
    },
    "connection": {
      "requirement": "optional"
    },
    "disposition_id": {
      "enum": {
        "1": {
          "name": "Resolved",
          "description": "The DNS request was successfully resolved."
        },
        "2": {
          "name": "Failed",
          "description": "The DNS request has failed."
        },
        "3": {
          "name": "Query",
          "description": "The DNS query request."
        }
      }
    },
    "proxy": {
      "requirement": "optional"
    },
    "query": {
      "requirement": "recommended"
    },
    "query_time": {
      "requirement": "optional"
    },
    "rcode": {
      "requirement": "optional"
    },
    "traffic": {
      "requirement": "optional"
    }
  }
}