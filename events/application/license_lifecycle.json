{
  "name": "License Lifecycle",
  "type": "license_lifecycle",
  "uid": 310,
  "extends": "license",
  "description": "License Lifecycle events report the installation, update, expiration, and removal of a license.",
  "attributes": {
    "outcome_id": {
      "enum": {
        "1": {
          "name": "Installed"
        },
        "2": {
          "name": "Removed"
        },
        "3": {
          "name": "Updated"
        },
        "4": {
          "name": "Expiring"
        },
        "5": {
          "name": "Expired"
        },
        "6": {
          "name": "No Subscription"
        }
      }
    },
    "days_left": {
      "description": "The number of days left before license expiration.",
      "requirement": "optional"
    }
  }
}