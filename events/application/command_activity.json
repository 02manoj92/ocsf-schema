{
  "name": "Command Activity",
  "type": "command_activity",
  "uid": 307,
  "extends": "application",
  "description": "Command Activity events report the state and status of commands.",
  "attributes": {
    "command_name": {
      "requirement": "required"
    },
    "command_ref_uid": {
      "requirement": "optional"
    },
    "command_uid": {
      "requirement": "recommended"
    },
    "interpreter": {
      "requirement": "recommended"
    },
    "outcome_id": {
      "enum": {
        "1": {
          "name": "Submitted"
        },
        "2": {
          "name": "Queued"
        },
        "3": {
          "name": "Started"
        },
        "4": {
          "name": "Cancel Requested"
        },
        "5": {
          "name": "Completed"
        },
        "6": {
          "name": "Canceled"
        },
        "7": {
          "name": "Error"
        },
        "8": {
          "name": "Rejected"
        }
      }
    },
    "severity_id": {
      "enum": {
        "1": {},
        "4": {}
      }
    }
  }
}