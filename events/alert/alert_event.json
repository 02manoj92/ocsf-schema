{
  "name": "Alert Events",
  "type": "alert_event",
  "extends": "alert",
  "description": "Alert events report detections and possible resolutions of threats, anomalies, and actions performed by security products.",
  "attributes": {
    "actor_process": {
      "requirement": "required"
    },
    "attacks": {
      "requirement": "optional"
    },
    "content_ver": {
      "requirement": "optional"
    },
    "cvssv2": {
      "requirement": "optional"
    },
    "detection_uid": {
      "requirement": "optional"
    },
    "reason": {
      "requirement": "optional"
    },
    "scan_uid": {
      "requirement": "optional"
    },
    "threat": {
      "requirement": "required"
    },
    "threats": {
      "requirement": "optional"
    }
  }
}