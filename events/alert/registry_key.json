{
  "name": "Registry Key Alert",
  "type": "registry_key_alert",
  "uid": 515,
  "extends": "alert_event",
  "description": "Registry Key Alert events report the detections and resolutions of registry key malicious access.",
  "attributes": {
    "access_mask": {
      "requirement": "optional"
    },
    "access_mask_ids": {
      "requirement": "optional",
      "enum": {
        "1": {
          "name": "Generic Read"
        },
        "2": {
          "name": "Generic Write"
        },
        "3": {
          "name": "Generic Execute"
        },
        "4": {
          "name": "Generic All"
        },
        "5": {
          "name": "Delete"
        },
        "6": {
          "name": "Write DAC"
        },
        "7": {
          "name": "Write Owner"
        },
        "16": {
          "name": "Read Attributes"
        },
        "17": {
          "name": "Write Attributes"
        },
        "18": {
          "name": "Read Control"
        },
        "19": {
          "name": "Access System Security"
        },
        "20": {
          "name": "Max Allowed"
        },
        "21": {
          "name": "Query Value"
        },
        "22": {
          "name": "Set Value"
        },
        "23": {
          "name": "Create Sub Key"
        },
        "24": {
          "name": "Enum Sub Keys"
        },
        "25": {
          "name": "Notify"
        },
        "26": {
          "name": "Create Link"
        }
      }
    },
    "activity": {
      "requirement": "optional"
    },
    "activity_id": {
      "description": "The registry key activity.",
      "requirement": "optional",
       "enum": {
        "1": {
          "name": "Created"
        },
        "2": {
          "name": "Read"
        },
        "3": {
          "name": "Modified"
        },
        "4": {
          "name": "Deleted"
        },
        "5": {
          "name": "Renamed"
        },
        "6": {
          "name": "Set Security"
        },
        "7": {
          "name": "Restored"
        }
      }
    },
    "create_mask": {
      "requirement": "optional"
    },
    "create_mask_id": {
      "description": "The Windows setting needed to create a registry key",
      "requirement": "recommended",
      "enum": {
        "0": {
          "name": "REG_OPTION_NON_VOLATILE",
          "description": "(Default) - This key is not volatile. The information is stored in a file and is preserved when the system is restarted."
        },
        "1": {
          "name": "REG_OPTION_VOLATILE",
          "description": "All keys created by the function are volatile. The information is stored in memory and is not preserved when the corresponding registry hive is unloaded."
        },
        "2": {
          "name": "REG_OPTION_CREATE_LINK",
          "description": "This key is a symbolic link."
        },
        "4": {
          "name": "REG_OPTION_BACKUP_RESTORE",
          "description": "Attempts to open the key with the access required to backup or restore the key."
        }
      }
    },
    "open_mask_id": {
      "description": "The Windows setting needed to open a registry key.",
      "requirement": "recommended",
      "enum": {
        "0": {
          "name": "NONE",
          "description": "Default"
        },
        "8": {
          "name": "REG_OPTION_OPEN_LINK",
          "description": "The key is a symbolic link"
        }
      }
    },
    "disposition_id": {
      "enum": {
        "1": {
          "name": "Allowed"
        },
        "2": {
          "name": "Blocked"
        },
        "3": {
          "name": "Command Script"
        },
        "4": {
          "name": "Corrected"
        },
        "5": {
          "name": "Partially Corrected"
        },
        "6": {
          "name": "Uncorrected"
        },
        "7": {
          "name": "Delayed",
          "description": "Requires reboot to finish the operation."
        },
        "8": {
          "name": "Deleted"
        },
        "9": {
          "name": "Restored"
        },
        "10": {
          "name": "Detected"
        }
      }
    },
    "reg_key": {
      "description": "The registry key that triggered the alert.",
      "requirement": "required"
    },
    "reg_key_result": {
      "description": "The resulting registry key; for example, if a Registry key operation is ALLOWED, the resulting registry key can be included in the event.",
      "requirement": "optional"
    }
  }
}