{
  "name": "Endpoint Kernel Alert",
  "type": "endpoint_kernel_alert",
  "uid": 208,
  "description": "Endpoint Kernel Alert events report the detections and resolutions of kernel resource malicious access.",
  "extends": "endpoint_alert",
  "see_also": [
    "kernel_activity"
  ],
  "attributes": {
    "activity": {
      "group": "primary",
      "requirement": "optional"
    },
    "activity_id": {
      "$include": "enums/kernel_activity.json",
      "group": "primary",
      "requirement": "recommended"
    },
    "disposition_id": {
      "enum": {
        "1": {
          "name": "Allowed"
        },
        "2": {
          "name": "Blocked"
        },
        "3": {
          "name": "Command Script Run"
        },
        "4": {
          "name": "Uncorrected"
        },
        "5": {
          "description": "Requires reboot to finish the operation.",
          "name": "Delayed"
        },
        "6": {
          "name": "Deleted"
        },
        "7": {
          "name": "Detected"
        }
      }
    },
    "kernel": {
      "description": "The kernel resource object that triggered the alert.",
      "group": "primary",
      "requirement": "required"
    }
  }
}
