{
  "name": "File Alert",
  "uid": 505,
  "type": "file_alert",
  "extends": "endpoint_alert",
  "description": "File Alert events report the detections and resolutions of file malicious activity.",
  "attributes": {
    "access_mask": {
      "requirement": "optional"
    },
    "access_mask_ids": {
      "requirement": "optional"
    },
    "activity": {
      "requirement": "optional"
    },
    "activity_id": {
      "description": "The file activity.",
      "requirement": "optional",
      "enum": {
        "1": {
          "name": "Created"
        },
        "2": {
          "name": "Read"
        },
        "3": {
          "name": "Modified"
        },
        "4": {
          "name": "Deleted"
        },
        "5": {
          "name": "Renamed"
        },
        "6": {
          "name": "Set Attributes"
        },
        "7": {
          "name": "Set Security"
        },
        "8": {
          "name": "Get Attributes"
        },
        "9": {
          "name": "Get Security"
        },
        "10": {
          "name": "Encrypted"
        },
        "11": {
          "name": "Decrypted"
        }
      }
    },
    "component": {
      "requirement": "optional"
    },
    "connection_ref_uid": {
      "requirement": "optional"
    },
    "create_mask": {
      "requirement": "optional"
    },
    "create_mask_id": {
      "description": "The Windows create file flag, applicable to System Activity File Create event.",
      "requirement": "optional",
      "enum": {
        "1": {
          "name": "Create New"
        },
        "2": {
          "name": "Create Always"
        },
        "3": {
          "name": "Open Existing"
        },
        "4": {
          "name": "Open Always"
        },
        "5": {
          "name": "Truncate Existing"
        }
      }
    },
    "disposition_id": {
      "enum": {
        "1": {
          "name": "Allowed"
        },
        "2": {
          "name": "Blocked"
        },
        "3": {
          "name": "Command Script Run"
        },
        "4": {
          "name": "Corrected"
        },
        "5": {
          "name": "Partially Corrected"
        },
        "6": {
          "name": "Uncorrected"
        },
        "7": {
          "name": "Delayed",
          "description": "Requires reboot to finish the operation."
        },
        "8": {
          "name": "Deleted"
        },
        "9": {
          "name": "Quarantined"
        },
        "10": {
          "name": "Restored"
        },
        "11": {
          "name": "Detected"
        },
        "12": {
          "name": "Exonerated",
          "description": "No longer suspicious (re-scored)."
        },
        "13": {
          "name": "Tagged",
          "description": "Marked with extended attributes."
        }
      }
    },
    "file": {
      "description": "The file that triggered the alert.",
      "requirement": "required"
    },
    "file_diff": {
      "requirement": "optional"
    },
    "file_result": {
      "description": "The resulting file object; for example, if a file operation is ALLOWED, the resulting file object can be included in the event.",
      "requirement": "optional"
    },
    "quarantine_uid": {
      "description": "If the event disposition is one of:<ul><li>[12] QUARANTINED</li><li>[13] RESTORED (from quarantine)</li></ul>include the unique identifier of the item that was quarantined or restored from quarantine.",
      "requirement": "optional"
    },
    "scan_uid": {
      "description": "The application-generated unique ID of the scan, if applicable, that caused the alert event.",
      "requirement": "optional"
    }
  },
  "see_also": [
    "file_activity",
    "folder_activity"
  ]
}