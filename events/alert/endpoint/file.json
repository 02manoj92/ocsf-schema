{
  "name": "File Alert",
  "type": "file_alert",
  "uid": 207,
  "description": "File Alert events report the detections and resolutions of file malicious activity.",
  "extends": "endpoint_alert",
  "see_also": [
    "file_activity",
    "folder_activity"
  ],
  "attributes": {
    "$include": "traits/file.json",
    "activity": {
      "group": "primary",
      "requirement": "optional"
    },
    "activity_id": {
      "$include": "enums/file_activity.json",
      "group": "primary",
      "requirement": "recommended"
    },
    "disposition_id": {
      "enum": {
        "1": {
          "name": "Allowed"
        },
        "2": {
          "name": "Blocked"
        },
        "3": {
          "name": "Command Script Run"
        },
        "4": {
          "name": "Corrected"
        },
        "5": {
          "name": "Partially Corrected"
        },
        "6": {
          "name": "Uncorrected"
        },
        "7": {
          "description": "Requires reboot to finish the operation.",
          "name": "Delayed"
        },
        "8": {
          "name": "Deleted"
        },
        "9": {
          "name": "Quarantined"
        },
        "10": {
          "name": "Restored"
        },
        "11": {
          "name": "Detected"
        },
        "12": {
          "description": "No longer suspicious (re-scored).",
          "name": "Exonerated"
        },
        "13": {
          "description": "Marked with extended attributes.",
          "name": "Tagged"
        }
      }
    },
    "quarantine_uid": {
      "description": "If the event disposition is one of:<ul><li>9 Quarantined</li><li>10 Restored (from quarantine)</li></ul>include the unique identifier of the item that was quarantined or restored from quarantine.",
      "group": "primary",
      "requirement": "optional"
    }
  }
}
