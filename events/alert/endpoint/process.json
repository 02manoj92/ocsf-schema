{
  "name": "Process Alert",
  "type": "process_alert",
  "uid": 218,
  "description": "Process Alert events report the detections and resolutions of process malicious activity.",
  "extends": "endpoint_alert",
  "see_also": [
    "process_activity",
    "module_activity",
    "module_alert"
  ],
  "attributes": {
    "activity": {
      "group": "primary",
      "requirement": "optional"
    },
    "activity_id": {
      "$include": "enums/process_activity.json",
      "group": "primary",
      "requirement": "recommended"
    },
    "actual_permissions": {
      "group": "primary",
      "requirement": "optional"
    },
    "disposition_id": {
      "enum": {
        "1": {
          "name": "Allowed"
        },
        "10": {
          "name": "Restored"
        },
        "11": {
          "name": "Detected"
        },
        "12": {
          "name": "Terminated"
        },
        "2": {
          "name": "Blocked"
        },
        "3": {
          "name": "Command Script Run"
        },
        "4": {
          "name": "Corrected"
        },
        "5": {
          "name": "Partially Corrected"
        },
        "6": {
          "name": "Uncorrected"
        },
        "7": {
          "description": "Requires reboot to finish the operation.",
          "name": "Delayed"
        },
        "8": {
          "name": "Deleted"
        },
        "9": {
          "name": "Quarantined"
        }
      }
    },
    "injection_type": {
      "group": "primary",
      "requirement": "optional"
    },
    "injection_type_id": {
      "group": "primary",
      "requirement": "optional"
    },
    "process": {
      "description": "The process that triggered the alert.",
      "group": "primary",
      "requirement": "required"
    },
    "quarantine_uid": {
      "description": "If the event id is one of:<ul><li>[12] Quarantined</li><li>[13] Restored</li></ul>the unique identifier of the item that was quarantined or restored from quarantine.",
      "group": "primary",
      "requirement": "optional"
    },
    "requested_permissions": {
      "group": "primary",
      "requirement": "optional"
    }
  }
}
