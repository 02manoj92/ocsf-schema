{
  "name": "Process Alert",
  "uid": 516,
  "type": "process_alert",
  "extends": "endpoint_alert",
  "description": "Process Alert events report the detections and resolutions of process malicious activity.",
  "attributes": {
    "activity": {
      "requirement": "optional"
    },
    "activity_id": {
      "description": "The process activity.",
      "requirement": "optional",
      "enum": {
        "0": {
          "name": "Unknown"
        },
        "1": {
          "name": "Launch"
        },
        "2": {
          "name": "Terminate"
        },
        "3": {
          "name": "Open"
        },
        "4": {
          "name": "Inject"
        },
        "5": {
          "name": "Set User ID"
        },
        "-1": {
          "name": "Other"
        }
      }
    },
    "actual_permissions": {
      "requirement": "optional"
    },
    "disposition_id": {
      "enum": {
        "1": {
          "name": "Allowed"
        },
        "2": {
          "name": "Blocked"
        },
        "3": {
          "name": "Command Script Run"
        },
        "4": {
          "name": "Corrected"
        },
        "5": {
          "name": "Partially Corrected"
        },
        "6": {
          "name": "Uncorrected"
        },
        "7": {
          "name": "Delayed",
          "description": "Requires reboot to finish the operation."
        },
        "8": {
          "name": "Deleted"
        },
        "9": {
          "name": "Quarantined"
        },
        "10": {
          "name": "Restored"
        },
        "11": {
          "name": "Detected"
        },
        "12": {
          "name": "Terminated"
        }
      }
    },
    "injection_type": {
      "requirement": "optional"
    },
    "injection_type_id": {
      "requirement": "optional"
    },
    "process": {
      "description": "The process that triggered the alert.",
      "requirement": "required"
    },
    "quarantine_uid": {
      "description": "If the event id is one of:<ul><li>[12] Quarantined</li><li>[13] Restored</li></ul>the unique identifier of the item that was quarantined or restored from quarantine.",
      "requirement": "optional"
    },
    "requested_permissions": {
      "requirement": "optional"
    },
    "resource": {
      "requirement": "required"
    }
  },
  "see_also": [
    "process_activity"
  ]
}