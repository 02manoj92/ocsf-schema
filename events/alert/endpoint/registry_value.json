{
  "name": "Registry Value Alert",
  "type": "registry_value_alert",
  "uid": 220,
  "description": "Registry Value Alert events report the detections and resolutions of registry value malicious access.",
  "extends": "endpoint_alert",
  "see_also": [
    "registry_key_alert",
    "registry_key_activity",
    "registry_value_activity"
  ],
  "attributes": {
    "$include": "traits/registry_value.json",
    "activity": {
      "group": "primary",
      "requirement": "optional"
    },
    "activity_id": {
      "$include": "enums/registry_value_activity.json",
      "group": "primary",
      "requirement": "recommended"
    },
    "disposition_id": {
      "enum": {
        "1": {
          "name": "Allowed"
        },
        "10": {
          "name": "Detected"
        },
        "2": {
          "name": "Blocked"
        },
        "3": {
          "name": "Command Script Run"
        },
        "4": {
          "name": "Corrected"
        },
        "5": {
          "name": "Partially Corrected"
        },
        "6": {
          "name": "Uncorrected"
        },
        "7": {
          "description": "Requires reboot to finish the operation.",
          "name": "Delayed"
        },
        "8": {
          "name": "Deleted"
        },
        "9": {
          "name": "Restored"
        }
      }
    }
  }
}
