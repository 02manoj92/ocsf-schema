{
  "name": "Endpoint Network Traffic Alert",
  "type": "endpoint_network_traffic_alert",
  "uid": 507,
  "extends": "endpoint_network_alert",
  "description": "Endpoint Network Traffic Alert events report the detections of threats in the network traffic data.",
  "attributes": {
    "connection": {
      "requirement": "optional"
    },
    "disposition_id": {
      "enum": {
        "1": {
          "name": "Allowed"
        },
        "2": {
          "name": "Blocked"
        },
        "3": {
          "name": "Dropped"
        },
        "4": {
          "name": "Deleted"
        },
        "5": {
          "name": "Isolated"
        }
      }
    },
    "traffic": {
      "requirement": "required"
    }
  }
}