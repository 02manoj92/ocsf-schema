{
  "name": "User Session Alert",
  "type": "user_session_alert",
  "uid": 518,
  "extends": "endpoint_alert",
  "description": "User Session Alert events report the detections and resolutions of session malicious activity.",
  "attributes": {
    "activity_id": {
      "description": "The ruser session activity.",
      "requirement": "optional",
      "enum": {
        "1": {
          "name": "Logon"
        },
        "2": {
          "name": "Logoff"
        }
      }
    },
    "disposition_id": {
      "enum": {
        "1": {
          "name": "Allowed"
        },
        "2": {
          "name": "Blocked"
        },
       "3": {
          "name": "Command Script Run"
        },
        "4": {
          "name": "Detected"
        },
        "5": {
          "name": "Terminated"
        }
      }
    },
    "session": {
      "requirement": "required"
    }
  }
}