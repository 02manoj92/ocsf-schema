{
  "name": "Authentication Session Alert",
  "uid": 518,
  "type": "authentication_session_alert",
  "extends": "endpoint_alert",
  "description": "Authentication Session Alert events report the detections and resolutions of session malicious activity.",
  "attributes": {
    "$include": "traits/authentication_session.json",
    "activity_id": {
      "description": "The authentication session activity.",
      "requirement": "optional",
      "enum": {
        "1": {
          "name": "Logon"
        },
        "2": {
          "name": "Logoff"
        }
      }
    },
    "disposition_id": {
      "enum": {
        "1": {
          "name": "Allowed"
        },
        "2": {
          "name": "Blocked"
        },
        "3": {
          "name": "Command Script Run"
        },
        "4": {
          "name": "Detected"
        },
        "5": {
          "name": "Terminated"
        }
      }
    }
  }
}