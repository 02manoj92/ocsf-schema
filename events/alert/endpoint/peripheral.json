{
  "name": "Peripheral Device Alert",
  "type": "peripheral_alert",
  "uid": 513,
  "extends": "endpoint_alert",
  "description": "Peripheral Device Alert events report the detections and resolutions of peripheral device activity.",
  "attributes": {
    "activity": {
      "requirement": "optional"
    },
    "activity_id": {
      "requirement": "recommended",
      "enum": {
        "1": {
          "name": "Read"
        },
        "2": {
          "name": "Write"
        },
        "3": {
          "name": "Delete"
        },
        "4": {
          "name": "Device Mount"
        },
        "5": {
          "name": "Execute"
        }
      }
    },
    "file": {
      "description": "The file that pertains to the alert event.",
      "requirement": "optional"
    },
    "disposition_id": {
      "enum": {
        "1": {
          "name": "Allowed"
        },
        "2": {
          "name": "Blocked"
        }
      }
    },
    "peripheral_device": {
      "description": "The peripheral device object that triggered the alert.",
      "requirement": "required"
    }
  }
}