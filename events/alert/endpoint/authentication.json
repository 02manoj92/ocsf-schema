{
  "name": "Endpoint Authentication Alert",
  "uid": 0,
  "type": "endpoint_authentication_alert",
  "extends": "endpoint_alert",
  "description": "Endpoint Authentication Alert events report the detections and resolutions of authentication session malicious activity.",
  "attributes": {
    "$include": "traits/authentication.json",
    "activity": {
      "requirement": "optional",
      "group": "primary"
    },
    "activity_id": {
      "requirement": "recommended",
      "$include": "enums/authentication.json",
      "group": "primary"
    },
    "outcome_id": {
      "enum": {
        "1": {
          "name": "Allowed"
        },
        "2": {
          "name": "Blocked"
        },
        "3": {
          "name": "Command Script Run"
        },
        "4": {
          "name": "Detected"
        },
        "5": {
          "name": "Terminated"
        }
      }
    }
  },
  "see_also": [
    "authentication_audit",
    "account_change_audit"
  ]
}