{
  "caption": "Unsuccessful Discovery",
  "category": "discovery",
  "uid": 4,
  "name": "discovery_no_result",
  "extends": "base_event",
  "description": "Unsuccessful Discovery events report unsuccessful attempts at Evidence of Compromise queries.",
  "profiles": [
    "host"
  ],
  "attributes": {
    "$include": [
      "profiles/host.json"
    ],
    "activity_id": {
      "enum": {
        "1": {
          "caption": "Does not exist",
          "description": "The target was not found."
        },
        "2": {
          "caption": "Error",
          "description": "The discovery attempt failed."
        },
        "3": {
          "caption": "Unsupported"
        }
      }
    },
    "request_obj": {
      "requirement": "optional",
      "group": "primary"
    }
  }
}