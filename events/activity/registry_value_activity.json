{
  "name": "Registry Value Activity",
  "type": "registry_value_activity",
  "uid": 420,
  "extends": "activity",
  "description": "Registry Value Activity events reports when a process performs an action on a Windows registry value.",
  "attributes": {
    "outcome_id": {
      "enum": {
        "1": {
          "name": "Get"
        },
        "2": {
          "name": "Set"
        },
        "3": {
          "name": "Modified"
        },
        "4": {
          "name": "Deleted"
        }
      }
    },
    "reg_value": {
      "requirement": "required"
    },
    "reg_value_result": {
      "requirement": "optional"
    }
  }
}