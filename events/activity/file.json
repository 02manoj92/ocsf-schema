{
  "name": "File Activity",
  "type": "file_activity",
  "uid": 407,
  "extends": "activity",
  "description": "File Activity events report when a process performs an action on a file.",
  "attributes": {
    "access_mask": {
      "requirement": "optional"
    },
    "access_mask_ids": {
      "requirement": "optional"
    },
    "connection_ref_uid": {
      "requirement": "optional"
    },
    "create_mask": {
      "requirement": "optional"
    },
    "create_mask_id": {
      "description": "The Windows setting needed when creating a file.",
      "requirement": "optional",
      "enum": {
        "1": {
          "name": "CREATE_NEW",
          "description": "Creates a new file, only if it does not already exist"
        },
        "2": {
          "name": "CREATE_ALWAYS",
          "description": "Creates a new file, always"
        },
        "3": {
          "name": "OPEN_EXISTING",
          "description": "Opens a file or device, only if it exists"
        },
        "4": {
          "name": "OPEN_ALWAYS",
          "description": "Opens a file, always"
        },
        "5": {
          "name": "TRUNCATE_EXISTING",
          "description": "Opens a file and truncates it so that its size is zero bytes, only if it exists"
        }
      }
    },
    "file": {
      "description": "The file that is the target of the operation.",
      "requirement": "required"
    },
    "file_diff": {
      "requirement": "optional"
    },
    "file_result": {
      "requirement": "optional"
    },
    "disposition_id": {
      "enum": {
        "1": {
          "name": "Created"
        },
        "2": {
          "name": "Read"
        },
        "3": {
          "name": "Modified"
        },
        "4": {
          "name": "Deleted"
        },
        "5": {
          "name": "Renamed"
        },
        "6": {
          "name": "Set Attributes"
        },
        "7": {
          "name": "Set Security"
        },
        "8": {
          "name": "Get Attributes"
        },
        "9": {
          "name": "Get Security"
        },
        "10": {
          "name": "Encrypted"
        },
        "11": {
          "name": "Decrypted"
        }
      }
    }
  }
}