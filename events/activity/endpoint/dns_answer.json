{
  "name": "Endpoint DNS Answer Activity",
  "uid": 409,
  "type": "endpoint_dns_answer_activity",
  "extends": "endpoint_activity",
  "description": "Endpoint DNS Answer Activity events report a DNS answer to a query initiated by some application running on an endpoint; and optionally, it may also include the DNS query information.",
  "attributes": {
    "activity": {
      "requirement": "optional"
    },
    "activity_id": {
      "description": "The DNS activity/query that triggered this answer.",
      "requirement": "optional",
      "enum": {
        "1": {
          "name": "Query"
        },
        "2": {
          "name": "Status"
        },
        "3": {
          "name": "Notify"
        },
        "4": {
          "name": "Update"
        },
        "5": {
          "name": "DNS Stateful Operations (DSO)"
        }
      }
    },
    "connection": {
      "requirement": "optional"
    },
    "disposition_id": {
      "enum": {
        "1": {
          "name": "Resolved"
        },
        "2": {
          "name": "Failed"
        }
      }
    },
    "dns_answers": {
      "requirement": "required"
    },
    "dns_query": {
      "description": "The Domain Name System (DNS) query that triggered this answer.",
      "requirement": "optional"
    },
    "end_time": {
      "description": "The DNS answer time (when the event contains the DNS query)."
    },
    "proxy": {
      "requirement": "optional"
    },
    "response_id": {
      "name": "DNS Response",
      "description": "The DNS response code.",
      "enum": {
        "0": {
          "name": "Unknown"
        },
        "1": {
          "name": "Format Error"
        },
        "2": {
          "name": "Server Failure"
        },
        "3": {
          "name": "Non-Existent Domain "
        },
        "4": {
          "name": "Not Implemented"
        },
        "5": {
          "name": "Query Refused"
        },
        "6": {
          "name": "Name Exists when it should not"
        },
        "7": {
          "name": "RR Set Exists when it should not "
        },
        "8": {
          "name": "RR Set that should exist does not "
        },
        "9": {
          "name": "Not Authorized  "
        },
        "10": {
          "name": "Name not contained in zone"
        },
        "11": {
          "name": "DSO-TYPE Not Implemented"
        },
        "16": {
          "name": "TSIG Signature Failure"
        },
        "17": {
          "name": "Key not recognized"
        },
        "18": {
          "name": "Signature out of time window"
        },
        "19": {
          "name": "Bad TKEY Mode"
        },
        "20": {
          "name": "Duplicate key name"
        },
        "21": {
          "name": "Algorithm not supported"
        },
        "22": {
          "name": "Bad Truncation"
        },
        "23": {
          "name": "Bad/missing Server Cookie"
        },
        "-1": {
          "name": "Other"
        }
      }
    },
    "time": {
      "description": "The DNS answer time, or if the event contains the DNS query, then this is the DNS query time."
    }
  }
}