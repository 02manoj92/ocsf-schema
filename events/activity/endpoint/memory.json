{
  "name": "Memory Activity",
  "uid": 410,
  "type": "memory_activity",
  "extends": "endpoint_activity",
  "description": "Memory Activity events report when a process performs internal memory allocation, modification, or other manipulation activities - such as a buffer overflow or turning off data execution protection (DEP) - that are not typical for a process.",
  "attributes": {
    "actual_permissions": {
      "requirement": "optional"
    },
    "disposition_id": {
      "enum": {
        "1": {
          "name": "Page Allocated"
        },
        "2": {
          "name": "Page Modified"
        },
        "3": {
          "name": "Page Deleted"
        },
        "4": {
          "name": "Buffer Overflow"
        },
        "5": {
          "name": "Disabled DEP",
          "description": "Data Execution Permission"
        },
        "6": {
          "name": "Enabled DEP",
          "description": "Data Execution Permission"
        }
      }
    },
    "requested_permissions": {
      "requirement": "optional"
    }
  },
  "see_also": [
    "memory_alert"
  ]
}