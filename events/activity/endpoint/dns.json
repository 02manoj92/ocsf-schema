{
  "name": "Endpoint DNS Activity",
  "uid": 409,
  "type": "endpoint_dns_activity",
  "extends": "endpoint_activity",
  "description": "Endpoint DNS Activity events report DNS queries and answers initiated by an application running on an endpoint.",
  "attributes": {
    "connection": {
      "requirement": "optional"
    },
    "disposition_id": {
      "enum": {
        "1": {
          "name": "Resolved",
          "description": "The DNS request was successfully resolved."
        },
        "2": {
          "name": "Failed",
          "description": "The DNS request has failed."
        },
        "3": {
          "name": "Query",
          "description": "The DNS query request."
        }
      }
    },
    "answer_time": {
      "requirement": "optional"
    },
    "answers": {
      "requirement": "recommended"
    },
    "query": {
      "requirement": "recommended"
    },
    "query_time": {
      "requirement": "optional"
    },
    "proxy": {
      "requirement": "optional"
    },
    "response_id": {
      "name": "DNS Response",
      "description": "The DNS response code.",
      "enum": {
        "0": {
          "name": "Unknown"
        },
        "1": {
          "name": "Format Error"
        },
        "2": {
          "name": "Server Failure"
        },
        "3": {
          "name": "Non-Existent Domain"
        },
        "4": {
          "name": "Not Implemented"
        },
        "5": {
          "name": "Query Refused"
        },
        "6": {
          "name": "Name Exists when it should not"
        },
        "7": {
          "name": "RR Set Exists when it should not"
        },
        "8": {
          "name": "RR Set that should exist does not"
        },
        "9": {
          "name": "Not Authorized "
        },
        "10": {
          "name": "Name not contained in zone"
        },
        "11": {
          "name": "DSO-TYPE Not Implemented"
        },
        "16": {
          "name": "TSIG Signature Failure"
        },
        "17": {
          "name": "Key not recognized"
        },
        "18": {
          "name": "Signature out of time window"
        },
        "19": {
          "name": "Bad TKEY Mode"
        },
        "20": {
          "name": "Duplicate key name"
        },
        "21": {
          "name": "Algorithm not supported"
        },
        "22": {
          "name": "Bad Truncation"
        },
        "23": {
          "name": "Bad/missing Server Cookie"
        },
        "-1": {
          "name": "Other"
        }
      }
   }
  }
}