{
  "name": "Endpoint Folder Activity",
  "type": "endpoint_folder_activity",
  "uid": 108,
  "description": "Endpoint Folder Activity events report when a process performs an action on a folder.",
  "extends": "endpoint_activity",
  "see_also": [
    "file_alert",
    "file_activity"
  ],
  "attributes": {
    "access_mask": {
      "group": "primary",
      "requirement": "optional"
    },
    "create_mask": {
      "group": "primary",
      "requirement": "optional"
    },
    "disposition_id": {
      "$include": "enums/file_activity.json",
      "description": "The disposition ID of the event.",
      "enum": {
        "12": {
          "name": "Mounted"
        },
        "13": {
          "name": "Unmounted"
        }
      }
    },
    "folder": {
      "group": "primary",
      "requirement": "required"
    },
    "folder_result": {
      "group": "primary",
      "requirement": "optional"
    }
  }
}
