{
  "name": "Folder Activity",
  "type": "folder_activity",
  "uid": 400,
  "extends": "endpoint_activity",
  "description": "Folder Activity events report when a process performs an action on a folder.",
  "attributes": {
    "access_mask": {
      "requirement": "optional"
    },
    "access_mask_ids": {
      "requirement": "optional"
    },
    "create_mask": {
      "requirement": "optional"
    },
    "create_mask_id": {
      "requirement": "optional",
      "enum": {
        "1": {
          "name": "CREATE_NEW",
          "description": "Creates a new file, only if it does not already exist"
        },
        "2": {
          "name": "CREATE_ALWAYS",
          "description": "Creates a new file, always"
        },
        "3": {
          "name": "OPEN_EXISTING",
          "description": "Opens a file or device, only if it exists"
        },
        "4": {
          "name": "OPEN_ALWAYS",
          "description": "Opens a file, always"
        },
        "5": {
          "name": "TRUNCATE_EXISTING",
          "description": "Opens a file and truncates it so that its size is zero bytes, only if it exists"
        }
      }
    },
    "folder": {
      "requirement": "required"
    },
    "folder_result": {
      "requirement": "optional"
    },
    "disposition_id": {
      "enum": {
        "1": {
          "name": "Created"
        },
        "2": {
          "name": "Read"
        },
        "3": {
          "name": "Modified"
        },
        "4": {
          "name": "Deleted"
        },
        "5": {
          "name": "Renamed"
        },
        "6": {
          "name": "Set Attributes"
        },
        "7": {
          "name": "Set Security"
        },
        "8": {
          "name": "Get Attributes"
        },
        "9": {
          "name": "Get Security"
        },
        "10": {
          "name": "Encrypted"
        },
        "11": {
          "name": "Decrypted"
        },
        "12": {
          "name": "Mounted"
        },
        "13": {
          "name": "Unmounted"
        }
      }
    }
  }
}