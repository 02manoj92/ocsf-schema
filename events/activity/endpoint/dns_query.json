      "requirement": "optional"
    },
    "dns_query": {
      "requirement": "required"
    },
    "disposition_id": {
      "enum": {
        "1": {
          "name": "Query"
        },
        "2": {
          "name": "Status"
        },
        "3": {
          "name": "Notify"
        },
        "4": {
          "name": "Update"
        },
        "5": {
          "name": "DNS Stateful Operations (DSO)"
        }
      }
    },
    "proxy": {
      "requirement": "optional"
     }
  }
