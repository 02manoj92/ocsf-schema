{
  "name": "DNS Query Activity",
  "type": "dns_query_activity",
  "uid": 401,
  "extends": "network_activity",
  "description": "DNS Query Activity events report a single DNS query.",
  "attributes": {
    "dns_query": {
      "requirement": "required"
    },
    "disposition_id": {
      "enum": {
        "1": {
          "name": "Query"
        },
        "2": {
          "name": "Status"
        },
        "3": {
          "name": "Notify"
        },
        "4": {
          "name": "Update"
        },
        "5": {
          "name": "DNS Stateful Operations (DSO)"
        }
      }
    },
    "server": {
      "requirement": "optional"
    }
  }
}