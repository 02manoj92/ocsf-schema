{
  "name": "Email Delivery Activity",
  "uid": 404,
  "type": "email_delivery_activity",
  "extends": "email_activity",
  "description": "Email Delivery events report the delivery status of emails.",
  "attributes": {
    "attempt": {
      "requirement": "recommended"
    },
    "banner": {
      "requirement": "optional"
    },
    "disposition_id": {
      "enum": {
        "1": {
          "name": "Delivered"
        },
        "2": {
          "name": "Failed"
        },
        "3": {
          "name": "Temporary Failure"
        }
      }
    },
    "dkim_signature": {
      "requirement": "recommended"
    },
    "email": {
      "requirement": "required"
    },
    "email_auth": {
      "requirement": "recommended"
    },
    "sender_hostname": {
      "requirement": "optional"
    },
    "sender_ip": {
      "requirement": "optional"
    }
  },
  "see_also": [
    "email_delivery_alert",
    "email_file_activity",
    "email_url_activity"
  ]
}