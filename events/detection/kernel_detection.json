{
  "name": "Kernel Detection",
  "type": "kernel_detection",
  "uid": 508,
  "extends": "detection_event",
  "description": "Kernel Detection events report the detection and resolution of kernel resource malicious access.",
  "attributes": {
    "activity": {
      "requirement": "optional"
    },
    "activity_id": {
      "description": "The kernel activity.",
      "requirement": "optional",
      "enum": {
        "0": {
          "name": "Unknown"
        },
        "1": {
          "name": "Create"
        },
        "2": {
          "name": "Delete"
        },
        "3": {
          "name": "Read"
        },
        "-1": {
          "name": "Other"
        }
      }
    },
    "kernel": {
      "description": "The kernel resource object that triggered the detection.",
      "requirement": "required"
    },
    "outcome_id": {
      "enum": {
        "1": {
          "name": "Allowed"
        },
        "2": {
          "name": "Blocked"
        },
        "3": {
          "name": "Command Script Run"
        },
        "4": {
          "name": "Uncorrected"
        },
        "5": {
          "name": "Delayed",
          "description": "Requires reboot to finish the operation."
        },
        "6": {
          "name": "Deleted"
        },
        "7": {
          "name": "Detected"
        }
      }
    }
  }
}