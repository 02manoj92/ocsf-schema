{
  "name": "Memory Detection",
  "type": "memory_detection",
  "uid": 509,
  "extends": "detection_event",
  "description": "Memory Detection events report the detection and resolution of malicious memory access.",
  "attributes": {
    "activity": {
      "requirement": "optional"
    },
    "activity_id": {
      "requirement": "recommended",
      "enum": {
        "0": {
          "name": "Unknown"
        },
        "1": {
          "name": "Page Allocated"
        },
        "2": {
          "name": "Page Modified"
        },
        "3": {
          "name": "Page Deleted"
        },
        "4": {
          "name": "Buffer Overflow"
        },
        "5": {
          "name": "Disabled DEP",
          "description": "Data Execution Permission"
        },
        "6": {
          "name": "Enabled DEP",
          "description": "Data Execution Permission"
        },
        "-1": {
          "name": "Other"
        }
      }
    },
    "disposition_id": {
      "enum": {
        "1": {
          "name": "Allowed"
        },
        "2": {
          "name": "Blocked"
        },
        "3": {
          "name": "Command Script Run"
        },
        "4": {
          "name": "Corrected"
        },
        "5": {
          "name": "Partially Corrected"
        },
        "6": {
          "name": "Uncorrected"
        },
        "7": {
          "name": "Delayed",
          "description": "Requires reboot to finish the operation."
        },
        "8": {
          "name": "Detected"
        }
      }
    }
  }
}