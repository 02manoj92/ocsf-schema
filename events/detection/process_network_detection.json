{
  "name": "Process Network Detection",
  "uid": 506,
  "type": "process_network_detection",
  "extends": "detection_event",
  "description": "Process Network Detection events report the detection and resolution of host network malicious access.",
  "attributes": {
    "actor_process": {
      "description": "The process that is establishing the network connection."
    },
    "connection": {
      "requirement": "required"
    },
    "outcome_id": {
      "enum": {
        "1": {
          "name": "Allowed"
        },
        "2": {
          "name": "Blocked"
        },
        "3": {
          "name": "Dropped"
        },
        "4": {
          "name": "Deleted"
        },
        "5": {
          "name": "Isolated"
        }
      }
    }
  },
  "proxy": {
    "requirement": "optional"
  }
}