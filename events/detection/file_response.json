{
  "name": "File Response",
  "type": "file_response",
  "uid": 506,
  "extends": "response",
  "description": "File Response events report file actions taken in response to a detection.",
  "attributes": {
    "file": {
      "description": "The file that pertains to the response action. ",
      "requirement": "required"
    },
    "outcome_id": {
      "enum": {
        "1": {
          "name": "Blocked"
        },
        "2": {
          "name": "Allowed"
        },
        "3": {
          "name": "No Action"
        },
        "4": {
          "name": "Logged"
        },
        "5": {
          "name": "Command Script"
        },
        "6": {
          "name": "Corrected"
        },
        "7": {
          "name": "Partially Corrected"
        },
        "8": {
          "name": "Uncorrected"
        },
        "10": {
          "name": "Delayed",
          "description": "Requires reboot to finish the operation."
        },
        "11": {
          "name": "Deleted"
        },
        "12": {
          "name": "Quarantined"
        },
        "13": {
          "name": "Restored"
        },
        "14": {
          "name": "Detected"
        },
        "15": {
          "name": "Exonerated",
          "description": "No longer suspicious (re-scored)."
        },
        "16": {
          "name": "Tagged",
          "description": "Marked with extended attributes."
        }
      }
    }
  }
}