{
  "name": "Email File Detection",
  "type": "email_file_detection",
  "uid": 502,
  "extends": "detection",
  "description": "Email File Detection events report the detection and resolution of threats within email file attachments.",
  "attributes": {
    "connection_ref_uid": {
      "requirement": "optional"
    },
    "email_uid": {
      "requirement": "required"
    },
    "file": {
      "description": "The file attachment that triggered the detection.",
      "requirement": "required"
    },
    "disposition_id": {
      "enum": {
        "1": {
          "name": "Allowed"
        },
        "2": {
          "name": "Blocked"
        },
        "3": {
          "name": "Isolated"
        },
        "4": {
          "name": "Quarantined"
        },
        "5": {
          "name": "Tagged"
        }
      }
    },
    "quarantine_uid": {
      "description": "The unique identifier of the item that was quarantined.",
      "requirement": "optional"
    },
    "reason_id": {
      "requirement": "required"
    },
    "threat": {
      "requirement": "required"
    }
  }
}