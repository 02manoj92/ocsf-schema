{
  "name": "Network Detection",
  "uid": 511,
  "type": "network_detection",
  "extends": "detection",
  "description": "Network Detection events report the detection and resolution of network threats.",
  "attributes": {
    "connection": {
      "requirement": "required"
    },
    "app_name": {
      "requirement": "optional"
    },
    "outcome_id": {
      "enum": {
        "1": {
          "name": "Allowed"
        },
        "2": {
          "name": "Blocked"
        },
        "3": {
          "name": "Dropped"
        },
        "4": {
          "name": "Deleted"
        },
        "5": {
          "name": "Isolated"
        }
      }
    },
    "rule": {
      "description": "The rule that triggered the network detection event.",
      "requirement": "recommended"
    },
    "threat": {
      "requirement": "required"
    }
  }
}