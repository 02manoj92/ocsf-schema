{
  "name": "Registry Value Detection",
  "type": "registry_value_detection",
  "uid": 516,
  "extends": "detection_event",
  "description": "Registry Value Detection events report the detection and resolution of registry value malicious access.",
  "attributes": {
    "activity": {
      "requirement": "optional"
    },
    "activity_id": {
      "description": "The registry value activity.",
      "requirement": "optional",
      "enum": {
        "0": {
          "name": "Unknown"
        },
        "1": {
          "name": "Get"
        },
        "2": {
          "name": "Set"
        },
        "3": {
          "name": "Deleted"
        },
        "-1": {
          "name": "Other"
        }
      }
    },
    "outcome_id": {
      "enum": {
        "1": {
          "name": "Blocked"
        },
        "2": {
          "name": "Allowed"
        },
        "3": {
          "name": "No Action"
        },
        "4": {
          "name": "Logged"
        },
        "5": {
          "name": "Command Script Run"
        },
        "6": {
          "name": "Corrected"
        },
        "7": {
          "name": "Partially Corrected"
        },
        "8": {
          "name": "Uncorrected"
        },
        "10": {
          "name": "Delayed",
          "description": "Requires reboot to finish the operation."
        },
        "11": {
          "name": "Deleted"
        },
        "13": {
          "name": "Restored"
        },
        "14": {
          "name": "Detected"
        }
      }
    },
    "reg_value": {
      "name": "Registry Value",
      "description": "The registry value that triggered the detection.",
      "requirement": "required"
    },
    "reg_value_result": {
      "name": "Registry Value Result",
      "description": "The resulting registry value; for example, if a Registry value operation is ALLOWED, the resulting registry value object can be included in the event.",
      "requirement": "optional"
    }
  }
}