{
  "name": "User Session Detection",
  "type": "session_detection",
  "uid": 518,
  "extends": "detection_event",
  "description": "User Session Detection events report the detection and resolution of session malicious activity.",
  "attributes": {
    "outcome_id": {
      "enum": {
        "1": {
          "name": "Allowed"
        },
        "2": {
          "name": "Blocked"
        },
       "3": {
          "name": "Command Script Run"
        },
        "4": {
          "name": "Detected"
        },
        "5": {
          "name": "Terminated"
        }
      }
    },
    "session": {
      "requirement": "required"
    }
  }
}