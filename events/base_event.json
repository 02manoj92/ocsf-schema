{
  "name": "Base Event",
  "type": "base_event",
  "description": "The base event is a generic concrete event and it also defines a set of attributes available in most event classes. As a generic event that does not belong to any event category, it could be used to log events that are not otherwise defined by the schema.",
  "attributes": {
    "$include": ["traits/occurrence.json", "traits/classification.json"],
    "enrichments": {
      "group": "context"
    },
    "message": {
      "requirement": "recommended",
      "group": "primary"
    },
    "metadata": {
      "requirement": "required",
      "group": "context"
    },
    "observables": {
      "requirement": "reserved",
      "group": "primary"
    },
    "origin": {
      "requirement": "required",
      "group": "context"
    },
    "raw_data": {
      "requirement": "reserved",
      "group": "context"
    },
    "unmapped": {
      "requirement": "reserved",
      "group": "context"
    }
  }
}
