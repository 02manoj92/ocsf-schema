{
  "caption": "System Activity classes",
  "category": "system",
  "extends": "base_event",
  "name": "system",
  "profiles": [
    "user",
    "host",
    "malware"
  ],
  "associations": {
    "origin.device": [
      "user"
    ],
    "user": [
      "origin.device"
    ]
  },
  "attributes": {
    "$include": [
      "profiles/host.json",
      "profiles/user.json",
      "profiles/malware.json"
    ],
    "actor_process": {
      "profile": null,
      "requirement": "required"
    },
    "device": {
      "group": "primary",
      "profile": null,
      "requirement": "required"
    },
    "is_user_present": {
      "profile": null
    },
    "user": {
      "profile": null,
      "requirement": "recommended"
    }
  }
}
