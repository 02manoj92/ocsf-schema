{
  "name": "Information Protection",
  "type": "information_protection",
  "uid": 608,
  "extends": "policy",
  "description": "Information Protection events report the detection and resolution of content policy violations.",
  "attributes": {
    "app_name": {
      "requirement": "recommended"
    },
    "owner_email": {
      "requirement": "recommended"
    },
    "owner_name": {
      "requirement": "recommended"
    },
    "detected_time": {
      "requirement": "recommended"
    },
    "num_violations": {
      "requirement": "recommended"
    },
    "disposition_id": {
      "description": "The disposition of the event. See the <strong>Response (response_action)</strong> for the detailed disposition.",
      "enum": {
        "1": {
          "name": "Allowed"
        },
        "2": {
          "name": "Blocked"
        },
        "3": {
          "name": "Quarantined"
        }
      }
    },
    "response_action": {
      "requirement": "recommended"
    }
  }
}