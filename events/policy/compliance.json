{
  "name": "Compliance",
  "type": "compliance",
  "uid": 601,
  "extends": "policy",
  "description": "Compliance events report the results of a compliance and remediation checks.",
  "attributes": {
    "type_id": {
      "description": "The compliance describes the type of a compliance check.",
      "requirement": "required",
      "enum": {
        "1": {
          "name": "Malware Protection Enforcement"
        },
        "2": {
          "name": "Patch Enforcement"
        },
        "3": {
          "name": "Service Pack Enforcement"
        },
        "4": {
          "name": "Firewall Enforcement"
        },
        "5": {
          "name": "Custom Rule Enforcement"
        },
        "6": {
          "name": "Configuration Enforcement"
        }
      }
    },
    "cvssv2": {
      "requirement": "recommended"
    },
    "outcome_id": {
      "enum": {
        "1": {
          "name": "Compliance Check Pass"
        },
        "2": {
          "name": "Compliance Check Fail"
        },
        "3": {
          "name": "Compliance Check Error"
        },
        "4": {
          "name": "Compliance Check Not Applicable"
        },
        "5": {
          "name": "Remediation Pass"
        },
        "6": {
          "name": "Remediation Fail"
        },
        "7": {
          "name": "Remediation Error"
        },
        "8": {
          "name": "Remediation Postponed"
        }
      }
    },
    "rule": {
      "requirement": "recommended"
    },
    "scan_uid": {
      "description": "The scan that is associated with this compliance event.",
      "requirement": "optional"
    }
  }
}